{
  "devDependencies": {
    "@asamuzakjp/dom-selector": "^6.4.7",
    "@testing-library/dom": "^9.3.4",
    "@types/chai": "^4.3.11",
    "@types/jsdom": "^21.1.6",
    "@types/mocha": "^10.0.6",
    "@types/node": "^22.14.0",
    "@types/sinon": "^17.0.3",
    "chai": "^5.1.0",
    "chai-dom": "^1.12.0",
    "esbuild": "^0.25.2",
    "esbuild-node-externals": "^1.18.0",
    "jsdom": "^24.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.3.0",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.1",
    "typescript": "^5.7.3"
  },
  "name": "campfire.js",
  "description": "a cozy web framework",
  "version": "4.0.0-rc1",
  "main": "dist/campfire.cjs.js",
  "module": "dist/campfire.esm.js",
  "types": "dist/campfire.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npx tsc --noEmit && npx mocha 'src/**/*.test.{js,ts}'",
    "mkdocs": "npx typedoc src/campfire.ts src/types.ts --cleanOutputDir --out site/docs/ --darkHighlightTheme material-palenight --customCss site/tsdoc_style.css && cp campfire.png site/docs/campfire.png",
    "build": "pnpm test && node esbuild.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xyzshantaram/campfire.git"
  },
  "keywords": [
    "web",
    "framework",
    "dom"
  ],
  "author": "shantaram",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xyzshantaram/campfire/issues"
  },
  "homepage": "https://xyzshantaram.github.io/campfire/",
  "type": "module",
  "pnpm.onlyBuiltDependencies": [
    "esbuild"
  ]
}
